{
  "rules": {
    "curly": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "linebreak-style": [true, "LF"],
    "max-line-length": [true, 80],
    "member-access": true,
    "member-ordering": [true, {"order": "fields-first"}],
    "no-any": true,
    "no-empty": true,
    "no-eval": true,
    "no-unused-new": true,
    "no-var-keyword": true,
    "only-arrow-functions": true,
    "prefer-for-of": true,
    "quotemark": [true, "single"],
    "semicolon": [true, "always"],
    "triple-equals": true,
    "whitespace": true
  }
}
